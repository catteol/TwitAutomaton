{
    "name": "dotnet container",
    "dockerComposeFile": [
        "../docker-compose.yml"
    ],
    "service": "dotnet",
    "remoteUser": "docker",
    "workspaceFolder": "/app",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-dotnettools.csharp",
                "msjsdiag.debugger-for-chrome",
                "editorconfig.editorconfig"
            ]
        }
    },
    "postCreateCommand": "dotnet restore",
    "shutdownAction": "none"
}